{
  "name": "web3dviewer",
  "productName": "Web3D Viewer",
  "version": "1.5.1",
  "description": "Preview.",
  "browser": "src/app.js",
  "scripts": {
    "build": "parcel build index.html",
    "package": "cnpm run package:windows && cnpm run package:mac && cnpm run package:linux",
    "dev": "parcel index.html --port 3000",
    "test": "node scripts/gen_test.js",
    "deploy": "cnpm run build && now dist --local-config now.json --prod",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "gltf",
    "three.js",
    "three",
    "3d",
    "model",
    "modeling",
    "webgl"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "dat.gui": "^0.7.5",
    "glob-to-regexp": "^0.4.0",
    "glsl-blend-soft-light": "^1.0.5",
    "glsl-film-grain": "^1.0.4",
    "gltf-validator": "^2.0.0-dev.3.2",
    "handlebars": "^4.7.0",
    "node-sass": "^4.13.1",
    "open": "6.0.0",
    "query-string": "^4.3.4",
    "simple-dropzone": "^0.7.0",
    "core-js": "^3.16.0",
    "regenerator-runtime": "^0.13.9",
    "jszip": "^3.7.0"
  },
  "devDependencies": {
    "chalk": "^2.1.0",
    "concurrently": "^3.5.0",
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.1",
    "node-fetch": "^1.7.3",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.2.1"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "assets",
        "staticOutDir": "assets"
      }
    ]
  },
  "main": "index.js",
  "directories": {
    "lib": "lib"
  }
}
